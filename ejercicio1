#include <stdio.h>
#include <conio.h>
#define Nombre "Pepe"
int main() {
	int a, b;
	float PI = 3.141592;
	char car = 'b';
	a = 3;
	b = (a+3) * 5;
	printf("Mi nombre es %s\n", Nombre);
	printf("Mi apellido es Peralta \n");
	printf("El valor de a es %d\nEl valor de la operacion es %d\n",a,b);
	printf("El valor de PI es %f\n", PI);
	printf("El valor con dos decimales de PI es %.2f\n", PI);
	printf("El valor del caracter es %c", car);
	getch();
	return 0;
}
